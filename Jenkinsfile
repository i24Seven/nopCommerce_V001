pipeline{
agent any
    stages{
    stage ("Build")
        {
        steps{
        echo "Bulding the Project...."
        echo "This is BUILD Stage..."
        }
    }
    stage("Test")
    {
        steps{
        echo "Testing the Project........"
        }
    }
    stage("Deploye")
      {
      steps{
      echo "Deploying the Project........"
      }
    }
}
}
